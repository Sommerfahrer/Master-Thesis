{
    "cSpell.enableFiletypes": [
        "bibtex",
        "bibtex-style",
        "latex"
    ],
    "cSpell.language": "de,en",
    "cSpell.words": [
        "Multithreading",
        "Raspbian",
        "Siemers",
        "WCET",
        "breakatwhitespace",
        "breaklines",
        "captionpos",
        "codegray",
        "codegreen",
        "codepurple",
        "hostapd",
        "keepspaces",
        "keywordstyle",
        "numbersep",
        "numberstyle",
        "showspaces",
        "showstringspaces",
        "systemctl",
        "tabsize"
    ],
    "latex-workshop.latex.autoBuild.run": "never",
    "latex-workshop.message.error.show": false,
    "latex-workshop.message.warning.show": false,
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-xelatex",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ]
        },
        {
            "name": "biber",
            "command": "biber",
            "args": [
                "%DOCFILE%"
            ]
        }
    ],
